{
    "name": "dmilho/wiredboot",
    "description": "Quickly scaffold Bootstrap 5 components using Laravel/Livewire.",
    "license": "MIT",
    "authors": [
        {
            "name": "David Milho",
            "email": "dmilho.dev@email.com",
            "homepage": "https://github.com/dmilho/wiredboot"
        }
    ],
    "homepage": "https://github.com/dmilho/wiredboot",
    "keywords": ["Laravel", "Wiredboot"],
    "require": {
        "illuminate/support": "~9"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "v7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Dmilho\\Wiredboot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dmilho\\Wiredboot\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dmilho\\Wiredboot\\WiredbootServiceProvider"
            ],
            "aliases": {
                "Wiredboot": "Dmilho\\Wiredboot\\Facades\\Wiredboot"
            }
        }
    },
"scripts": {
        "post-install-cmd": [
            "php artisan vendor:publish --tag=wiredboot.config --ansi --force"
        ]
    }
}
